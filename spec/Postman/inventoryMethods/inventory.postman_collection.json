					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Response has all required properties and valid data', () => {\r",
									"    const responseJson = pm.response.json();\r",
									"\r",
									"    pm.expect(responseJson).to.have.property('result');\r",
									"    pm.expect(responseJson.result).to.eql('ok');\r",
									"\r",
									"    pm.expect(responseJson).to.have.property('data');\r",
									"    pm.expect(responseJson.data).to.be.an('object');\r",
									"\r",
									"    pm.expect(responseJson.data).to.have.property('monsters');\r",
									"    pm.expect(responseJson.data.monsters).to.be.an('array');\r",
									"\r",
									"    responseJson.data.monsters.forEach((monster, index) => {\r",
									"        pm.expect(monster).to.have.property('id').that.is.a('number');\r",
									"        pm.expect(monster).to.have.property('name').that.is.a('string');\r",
									"        pm.expect(monster).to.have.property('element').that.is.a('string');\r",
									"        pm.expect(monster).to.have.property('status').that.is.a('string');\r",
									"        pm.expect(monster).to.have.property('level').that.is.a('number');\r",
									"        pm.expect(monster).to.have.property('current_hp').that.is.a('number');\r",
									"        pm.expect(monster).to.have.property('max_HP').that.is.a('string'); \r",
									"        pm.expect(monster).to.have.property('ATK').that.is.a('string'); \r",
									"        pm.expect(monster).to.have.property('DEF').that.is.a('string'); \r",
									"        pm.expect(monster).to.have.property('asset').that.is.a('string');\r",
									"        pm.expect(monster.asset).to.include('../../assets/characters/'); \r",
									"    });\r",
									"\r",
									"    pm.expect(responseJson.data).to.have.property('inventory');\r",
									"    pm.expect(responseJson.data.inventory).to.be.an('array');\r",
									"\r",
									"    responseJson.data.inventory.forEach((item, index) => {\r",
									"        pm.expect(item).to.have.property('id').that.is.a('number');\r",
									"        pm.expect(item).to.have.property('user_id').that.is.a('number');\r",
									"        pm.expect(item).to.have.property('resource_id').that.is.a('number');\r",
									"        pm.expect(item).to.have.property('resource_amount').that.is.a('number');\r",
									"        pm.expect(item).to.have.property('name').that.is.a('string');\r",
									"        pm.expect(item).to.have.property('cost').that.is.a('number');\r",
									"        pm.expect(item).to.have.property('exchange_cost'); \r",
									"        pm.expect(item).to.have.property('image').that.is.a('string');\r",
									"        pm.expect(item.image).to.include('../../assets/img/'); \r",
									"    });\r",
									"\r",
									"    pm.expect(responseJson.data).to.have.property('balance').that.is.a('number');\r",
									"    pm.expect(responseJson.data.balance).to.be.at.least(0); \r",
									"    console.log('Balance:', responseJson.data.balance);\r",
									"});\r",
									""
