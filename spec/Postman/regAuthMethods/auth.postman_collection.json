				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const login = pm.globals.get('login');\r",
							"const password = pm.globals.get('password');\r",
							"const rnd = Math.round(Math.random() * 100000);\r",
							"const hash = CryptoJS.MD5(CryptoJS.MD5(login + password).toString() + rnd).toString();\r",
							"\r",
							"pm.globals.set('rnd', rnd);\r",
							"pm.globals.set('hash', hash);\r",
							"\r",
							"const key1 = pm.environment.get(\"key1\");\r",
							"pm.collectionVariables.set('key2', 10001);\r",
							"\r",
							"const key2 = pm.collectionVariables.get('key2');\r",
							"\r",
							"console.log(key1, key2);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test('check request status is 200', () => {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"The response has all properties\", () => {\r",
							"    const responseJson = pm.response.json();\r",
							"    pm.expect(responseJson.result).to.eql('ok');\r",
							"    pm.expect(responseJson.data).to.be.a('object');\r",
							"    pm.expect(responseJson.data.id).to.be.a('number');\r",
							"    pm.expect(responseJson.data.name).to.eql('user');\r",
							"    pm.expect(responseJson.data.token).to.have.lengthOf(32);\r",
							"    pm.expect(responseJson.data.x).to.be.a('number');\r",
							"    pm.expect(responseJson.data.y).to.be.a('number');\r",
							"});"
						],
