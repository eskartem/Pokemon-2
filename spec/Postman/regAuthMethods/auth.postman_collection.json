							"});\r",
							"\r",
							"pm.test(\"Response contains error for wrong login or password\", () => {\r",
							"    const responseJson = pm.response.json();\r",
							"\r",
							"    pm.expect(responseJson).to.be.an(\"object\");\r",
							"    pm.expect(responseJson).to.have.property(\"result\", \"error\");\r",
							"\r",
							"\r",
